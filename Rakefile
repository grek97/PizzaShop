require "./app"
require "sinatra/activerecord/rake" # подключает это пространство из гемов которые мы подключили при установке sintara and activerecord
# Rakefile = нужен для команды rake, для исполнения миграции, просто руби файл
# с синтаксисом ruby, подключает разные name space 
# здесь он подключает нашу программу загружает файлы app,rb 
 